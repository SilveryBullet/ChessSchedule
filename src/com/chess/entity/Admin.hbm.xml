<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.chess.entity">

	<class name="Admin" table="chess_admin">
		<id name="id" type="long" column="chess_id">
			<generator class="native" />
		</id>
		
		<property name="loginName" type="string" length="50" column="chess_login_name"/>	
		<property name="password" type="string" length="50" column="chess_password"/>	
		<property name="userName" type="string" length="50" column="chess_user_name"/>		
		<property name="state" type="string" length="20" column="chess_state"/>
		<property name="privilege" type="string" length="20" column="chess_privilege" />
		
	</class>

</hibernate-mapping>

