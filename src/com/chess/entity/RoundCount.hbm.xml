<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.chess.entity">

	<class name="RoundCount" table="chess_round_count">
		<id name="id" type="long" column="chess_id">
			<generator class="native" />
		</id>
		
		<property name="roundNumber" type="long" length="8" column="chess_round_number"/>	
		<property name="playerNumber" type="long" length="8" column="chess_player_number"/>	
		
	</class>

</hibernate-mapping>

